<h1>Registration</h1>
<%= form_for @customer do |f|%>

<%= f.label :name %> </br>
<%= f.text_field :name %></br>

<%= f.label :password %></br>
<%=f.text_field :password %></br>

<%= f.label :address %></br>
<%=f.text_field :address %></br>

<%= f.label :ward %></br>
<%=f.text_field :ward %></br>

<%= f.submit %>
<%end%> 